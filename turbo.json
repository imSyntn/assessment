{
  "$schema": "https://turborepo.dev/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"],
      "env": ["DATABASE_URL", "GOOGLE_GENERATIVE_AI_API_KEY"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["DATABASE_URL", "GOOGLE_GENERATIVE_AI_API_KEY"]
    }
  }
}
